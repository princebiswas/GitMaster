package InterfaceConcept;

class ABC
{
	
	public void AbcMethod()
	{
		System.out.println("calling Abc Method");
	}
	 class BCD
	{
		public void BCDMethod()
		{
			System.out.println("Calling BCD MEthod");
		}
	}
}

interface Tringles
{
	public void TringleShape();
	interface Squares
	{
		public void SquareShape();
	}
}

class Mul implements Tringles,Tringles.Squares
{

	@Override
	public void SquareShape() {
		// TODO Auto-generated method stub
		System.out.println("Calling Square Shape");
	}

	@Override
	public void TringleShape() {
		// TODO Auto-generated method stub
		System.out.println("Calling Tringle Shape");
	}
	
}



class EFG
{
	public void Test1()
	{
		System.out.println("Calling Test1 ");
	}
	
	interface FGH
	{
		public void Test2(); //unimplemented method
	}
}

class Shapes extends EFG implements EFG.FGH 
{

	@Override
	public void Test2() {
		// TODO Auto-generated method stub
		System.out.println("Implemnts Test2");
	}
	
}


public class NestedConcept {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Shapes s = new Shapes();
		s.Test2();
		s.Test1();
		
		EFG e = new EFG();
		e.Test1();
		
		Mul m = new Mul();
		m.TringleShape();
		m.SquareShape();
		
		
		//driver.manage().window().maximi
		
		ABC a = new ABC();
		a.AbcMethod();
		
		ABC.BCD ab =  a.new BCD();
		
		ab.BCDMethod();

	}

}



