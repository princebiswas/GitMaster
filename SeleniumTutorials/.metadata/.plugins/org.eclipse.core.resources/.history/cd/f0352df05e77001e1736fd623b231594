package StaticConcept;

class Shape
{
	String shape= "Tringle";
}



public class StaticVariableMethod {
	
	static String Color = "Blue";// static variable

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		//StaticVariableMethod s = new StaticVariableMethod();
		
		System.out.println(StaticVariableMethod.Color);
		
		Shape s = new Shape();
		System.out.println(s.shape);
		

	}

}
